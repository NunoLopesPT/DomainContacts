{
    "name": "nuno/domain-contacts",
    "description": "PSR-4 Package for Contacts Management Backend with Laravel using API",
    "license": "MIT",
    "authors": [
        {
            "name": "Nuno Lopes",
            "email": "nunolopes96pt@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "NunoLopes\\DomainContacts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NunoLopes\\Tests\\DomainContacts\\": "tests/"
        }
    },
    "require": {
        "php": "~7.2",
        "illuminate/database": "^5.8",
        "illuminate/validation": "^5.8",
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.2"
    },
    "scripts": {
        "migrations": "NunoLopes\\DomainContacts\\Utilities\\Composer\\RunScripts::migrate",
        "seed"      : "NunoLopes\\DomainContacts\\Utilities\\Composer\\RunScripts::seed",
        "migrations-rollback": "NunoLopes\\DomainContacts\\Utilities\\Composer\\RunScripts::rollback"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
