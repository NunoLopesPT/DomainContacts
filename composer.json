{
    "name": "nuno/domain-contacts",
    "description": "PSR-4 Package for Contacts Management Backend with Laravel using API",
    "license": "MIT",
    "authors": [
        {
            "name": "Nuno Lopes",
            "email": "nunolopes96pt@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "NunoLopes\\DomainContacts\\": "src/"
        }
    },
    "require": {
        "php": "~7.2",
        "illuminate/database": "^5.8",
        "illuminate/validation": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "scripts": {
        "migrations": "NunoLopes\\DomainContacts\\Utilities\\Composer\\Migrations::run",
        "migrations-rollback": "NunoLopes\\DomainContacts\\Utilities\\Composer\\Migrations::rollback"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
